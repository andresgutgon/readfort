{
  "semi": false,
  "jsxSingleQuote": true,
  "singleQuote": true,
  "trailingComma": "all",
  "plugins": ["@ianvs/prettier-plugin-sort-imports"],
  "importOrderParserPlugins": ["typescript", "jsx", "importAssertions"],
  "importOrder": [
    "^react$",
    "^react-dom(.*)$",
    "lodash",
    "class-variance-authority",
    "^lucide(.*)$",
    "^@radix(.*)$",
    "^@tanstack(.*)$",
    "^@bookfort/ui/(.*)$",
    "",
    "<THIRD_PARTY_MODULES>",
    "",
    "^$/(.*)$",
    "",
    "^[.]"
  ],
  "importOrderTypeScriptVersion": "5.0.0"
}
